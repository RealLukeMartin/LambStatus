{
  "name": "lamb-status-frontend",
  "version": "0.3.1",
  "description": "Frontend for LambStatus",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "lint": "eslint src",
    "build": "better-npm-run build",
    "build:dev": "better-npm-run build:dev",
    "start": "npm run start:admin",
    "start:admin": "better-npm-run start admin",
    "start:status": "better-npm-run start status",
    "test": "better-npm-run test",
    "test:dev": "npm run test -- --watch",
    "deploy": "better-npm-run deploy",
    "release": "better-npm-run release",
    "release:force": "npm run release -- --force"
  },
  "betterScripts": {
    "build": {
      "command": "npm run clean && babel-node bin/fetch-settings.js && babel-node bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "build:dev": {
      "command": "babel-node bin/fetch-settings.js && babel-node bin/compile",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start": {
      "command": "npm run clean && babel-node bin/fetch-settings.js && babel-node bin/server",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "deploy": {
      "command": "npm run build && babel-node bin/upload-dist",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "test": {
      "command": "babel-node ./node_modules/karma/bin/karma start build/karma.conf",
      "env": {
        "NODE_ENV": "test",
        "DEBUG": "app:*"
      }
    },
    "release": {
      "command": "npm run build && babel-node bin/release-dist",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ks888/LambStatus.git"
  },
  "author": "Kishin Yagami <k.yagami.suou@gmail.com>",
  "license": "Apache-2.0",
  "dependencies": {
    "amazon-cognito-identity-js": "^1.8.0",
    "aws-sdk": "^2.6.1",
    "babel-cli": "^6.5.1",
    "babel-core": "^6.3.17",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-runtime": "^6.3.13",
    "babel-polyfill": "^6.9.0",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "babel-register": "^6.3.13",
    "babel-runtime": "^6.3.19",
    "better-npm-run": "0.0.10",
    "c3": "^0.4.11",
    "classnames": "^2.2.5",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.23.0",
    "cssnano": "^3.3.2",
    "debug": "^2.2.0",
    "dialog-polyfill": "^0.4.4",
    "dotenv": "^2.0.0",
    "extract-text-webpack-plugin": "^1.0.0",
    "file-loader": "^0.9.0",
    "fs-extra": "^0.30.0",
    "history": "^2.0.0",
    "html-webpack-plugin": "^2.7.1",
    "imports-loader": "^0.6.5",
    "ip": "^1.1.2",
    "json-loader": "^0.5.4",
    "linkifyjs": "^2.1.3",
    "mime": "^1.3.4",
    "moment-timezone": "^0.5.5",
    "node-sass": "^3.7.0",
    "normalize.css": "^4.1.1",
    "object-assign": "^4.1.1",
    "postcss-loader": "^0.9.0",
    "react": "^15.0.0",
    "react-addons-css-transition-group": "^15.3.1",
    "react-addons-transition-group": "^15.3.1",
    "react-addons-update": "^15.3.1",
    "react-datepicker": "^0.44.0",
    "react-dom": "^15.0.0",
    "react-redux": "^4.0.0",
    "react-router": "^2.2.0",
    "react-router-redux": "^4.0.0",
    "react-tooltip": "^3.2.10",
    "redux": "^3.0.0",
    "redux-thunk": "^2.0.0",
    "rimraf": "^2.5.1",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.0",
